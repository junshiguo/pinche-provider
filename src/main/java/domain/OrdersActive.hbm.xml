<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="domain">
    <class name="OrdersActive" table="orders_active">
        <id name="orderId" column="order_id" type="string">
            <generator class="assigned"/>
        </id>
        <property name="requestId1" column="request_id1" type="string"/>
        <property name="requestId2" column="request_id2" type="string"/>
        <property name="userId1" column="user_id1" type="string" />
        <property name="userId2" column="user_id2" type="string"/>
        <property name="confirmedUser1" column="confirmed_user1" type="byte"/>
        <property name="confirmedUser2" column="confirmed_user2" type="byte"/>
        <property name="orderTime" column="order_time" type="timestamp"/>
         <property name="savePercent" column="save_percent" type="double"/>
        <property name="routePoint1" column="route_point1" type="string"/>
        <property name="routePoint2" column="route_point2" type="string"/>
        <property name="routePoint3" column="route_point3" type="string"/>
        <property name="routePoint4" column="route_point4" type="string"/>
    </class>
</hibernate-mapping>